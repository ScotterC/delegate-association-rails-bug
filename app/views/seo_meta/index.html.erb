<h1>Listing seo_meta</h1>

<table>
  <tr>
    <th>Browser title</th>
    <th>Meta keywords</th>
    <th>Meta description</th>
    <th>Meta author</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @seo_meta.each do |seo_metum| %>
  <tr>
    <td><%= seo_metum.browser_title %></td>
    <td><%= seo_metum.meta_keywords %></td>
    <td><%= seo_metum.meta_description %></td>
    <td><%= seo_metum.meta_author %></td>
    <td><%= link_to 'Show', seo_metum %></td>
    <td><%= link_to 'Edit', edit_seo_metum_path(seo_metum) %></td>
    <td><%= link_to 'Destroy', seo_metum, :method => :delete, :data => { :confirm => 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

